{
  "env": {
      "browser": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": [
      "error",
      { 
        "ignore": [
          "navigation"
        ]
      }
    ],
    "no-use-before-define": 0,
    "no-console": 0,
    "global-require": 0
  }
}